{
    "name": "@getrocket/dev-mcp",
    "version": "0.1.0",
    "description": "Consolidated MCP server for Rocket dev tools (Postgres, ClickHouse, GraphQL, BigQuery)",
    "main": "build/index.js",
    "bin": {
        "dev-mcp": "build/index.js"
    },
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.18.0"
    },
    "scripts": {
        "clean": "rimraf build",
        "build": "yarn clean && tsc -p .",
        "start": "node build/index.js",
        "dev": "tsx src/index.ts"
    },
    "dependencies": {
        "@clickhouse/client": "^1.17.0",
        "@google-cloud/bigquery": "^8.1.1",
        "@modelcontextprotocol/sdk": "^1.26.0",
        "pg": "^8.18.0",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@types/node": "^22.7.5",
        "@types/pg": "^8.16.0",
        "rimraf": "^6.1.2",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "files": [
        "build",
        "package.json",
        "README.md"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    }
}
